{
    "name": "azimut",
    "version": "1.6.0",
    "description": "Logiciel de guidage pour un service d'incendie",
    "main": "main.js",
    "scripts": {
        "start": "npm run test && npm run jsinspect && webpack-dev-server",
        "dev": "webpack-dev-server --inline --progress --config webpack.config.js",
        "release": "webpack --config=webpack.config.js",
        "jsinspect": "jsinspect ./app",
        "clean": "rimraf dist",
        "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
        "lint": "eslint ./app/**/**.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "coverage": "jest --coverage",
        "precommit": "cross-env npm run lint && npm test",
        "prepush": "cross-env npm run lint && npm test",
        "webpack": "webpack",
        "electron": "webpack-dev-server && electron .",
        "po": "node server.js",
        "jsdoc": "jsdoc -r ./app"
    },
    "author": "Kliment Petrov <kleopetroff@gmail.com>",
    "license": "MIT",
    "jest": {
        "setupFiles": [
            "<rootDir>/config/jest/shim.js"
        ],
        "collectCoverageFrom": [
            "app/**/*.{js,jsx,ts,tsx}"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/assetsTransformer.js",
            "\\.(css|less)$": "<rootDir>/config/jest/assetsTransformer.js"
        },
        "testMatch": [
            "**/?(*.)(spec|test).js?(x)"
        ],
        "transform": {
            "\\.js$": "babel-jest"
        }
    },
    "dependencies": {
        "@turf/line-intersect": "^6.0.0",
        "axios": "^0.17.1",
        "axios-mock-adapter": "^1.14.1",
        "babel-polyfill": "^6.26.0",
        "bootstrap": "^4.0.0",
        "concurrently": "^3.5.1",
        "cross-env": "5.1.2",
        "dialog": "^0.3.1",
        "electron-menu-react": "^0.1.0",
        "electron-prompt": "^0.5.0",
        "electron-squirrel-startup": "^1.0.0",
        "electron-winstaller": "^2.6.4",
        "geolib": "^2.0.24",
        "jquery": "^3.3.1",
        "jsdoc": "^3.5.5",
        "jsinspect": "^0.12.7",
        "openlayers": "^4.6.4",
        "popper.js": "^1.13.0",
        "react": "16.2.0",
        "react-dom": "16.2.0",
        "react-electron-menu": "^0.3.0",
        "react-hot-loader": "4.0.0-beta.16",
        "react-loadable": "^5.3.1",
        "react-native-prompt": "^1.0.0",
        "react-router": "^4.2.0",
        "react-router-dom": "4.2.2",
        "sinon": "^4.4.2",
        "turf-linestring": "^1.0.2"
    },
    "devDependencies": {
        "autoprefixer": "^8.0.0",
        "babel": "^6.23.0",
        "babel-cli": "6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "8.1.2",
        "babel-jest": "22.0.6",
        "babel-loader": "^7.1.3",
        "babel-plugin-transform-flow-strip-types": "^6.22.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "6.24.1",
        "babel-preset-react-boilerplate": "1.1.1",
        "babel-preset-react-hmre": "1.1.1",
        "babel-preset-stage-2": "6.24.1",
        "canvas": "^1.6.9",
        "copy-webpack-plugin": "4.3.0",
        "css-loader": "0.28.8",
        "electron": "^1.8.2",
        "electron-default-menu": "^1.0.1",
        "electron-packager": "^11.0.1",
        "enzyme": "3.3.0",
        "enzyme-adapter-react-16": "1.1.1",
        "eslint": "4.13.0",
        "eslint-config-airbnb": "16.1.0",
        "eslint-loader": "1.9.0",
        "eslint-plugin-import": "2.8.0",
        "eslint-plugin-jsx-a11y": "6.0.3",
        "eslint-plugin-react": "7.5.1",
        "express": "^4.16.2",
        "extract-text-webpack-plugin": "3.0.2",
        "file-loader": "1.1.6",
        "html-webpack-plugin": "2.30.1",
        "jest": "22.1.0",
        "node-sass": "^4.7.2",
        "open-browser-webpack-plugin": "0.0.5",
        "react-addons-test-utils": "15.6.2",
        "react-dialog": "^1.0.2",
        "react-test-renderer": "16.2.0",
        "rimraf": "2.6.2",
        "sass-loader": "6.0.6",
        "style-loader": "0.19.1",
        "url-loader": "0.6.2",
        "webpack": "^3.11.0",
        "webpack-dev-middleware": "^3.0.0",
        "webpack-dev-server": "2.9.6",
        "webpack-hot-middleware": "^2.21.2",
        "webpack-target-electron-renderer": "^0.4.0"
    }
}
