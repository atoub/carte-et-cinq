{
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "globalReturn": false,
            "jsx": true,
            "experimentalObjectRestSpread": false
        }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-uses-vars": 1,
        "react/jsx-uses-react": 1,
        "react/react-in-jsx-scope": 1,
        "comma-dangle": [
            2,
            "always-multiline"
        ],
        "no-cond-assign": [
            1,
            "always"
        ],
        "no-console": 0,
        "no-constant-condition": 1,
        "no-control-regex": 0,
        "no-debugger": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 0,
        "no-empty": 1,
        "no-ex-assign": 1,
        "no-extra-boolean-cast": 1,
        "no-extra-parens": [
            1,
            "all"
        ],
        "no-extra-semi": 2,
        "no-func-assign": 1,
        "no-inner-declarations": [
            1,
            "both"
        ],
        "no-invalid-regexp": 0,
        "no-irregular-whitespace": 0,
        "no-negated-in-lhs": 0,
        "no-obj-calls": 2,
        "no-regex-spaces": 0,
        "no-sparse-arrays": 1,
        "no-unreachable": 2,
        "use-isnan": 1,
        "valid-jsdoc": 2,
        "valid-typeof": 2,
        "no-unexpected-multiline": 1,
        "accessor-pairs": 0,
        "block-scoped-var": 0,
        "complexity": 0,
        "consistent-return": 0,
        "curly": [
            2,
            "all"
        ],
        "default-case": 2,
        "dot-notation": 0,
        "dot-location": 0,
        "eqeqeq": 0,
        "guard-for-in": 1,
        "no-alert": 0,
        "no-caller": 2,
        "no-div-regex": 0,
        "no-else-return": 0,
        "no-empty-label": 0,
        "no-eq-null": 0,
        "no-eval": 1,
        "no-extend-native": 0,
        "no-extra-bind": 1,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implicit-coercion": [
            1,
            {
                "boolean": true,
                "number": true,
                "string": true
            }
        ],
        "no-implied-eval": 1,
        "no-invalid-this": 2,
        "no-iterator": 0,
        "no-labels": 0,
        "no-lone-blocks": 2,
        "no-loop-func": 1,
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-native-reassign": 1,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-new": 1,
        "no-octal-escape": 0,
        "no-octal": 1,
        "no-param-reassign": 1,
        "no-process-env": 1,
        "no-proto": 1,
        "no-redeclare": 1,
        "no-return-assign": 0,
        "no-script-url": 2,
        "no-self-compare": 1,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unused-expressions": 1,
        "no-useless-call": 1,
        "no-void": 1,
        "no-warning-comments": [
            1,
            {
                "terms": [
                    "todo",
                    " TODO"
                ],
                "location": "anywhere"
            }
        ],
        "no-with": 2,
        "radix": 0,
        "vars-on-top": 0,
        "wrap-iife": 0,
        "yoda": 0,
        "strict": 1,
        "init-declarations": 0,
        "no-catch-shadow": 1,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow-restricted-names": 1,
        "no-shadow": [
            1,
            {
                "hoist": "all"
            }
        ],
        "no-undef-init": 2,
        "no-undef": 0,
        "no-undefined": 0,
        "no-unused-vars": 1,
        "no-use-before-define": 0,
        "callback-return": 0,
        "handle-callback-err": 0,
        "no-mixed-requires": 0,
        "no-new-require": 0,
        "no-path-concat": 0,
        "no-process-exit": 0,
        "no-restricted-modules": 0,
        "no-sync": 0,
        "array-bracket-spacing": 0,
        "brace-style": [
            2,
            "1tbs",
            {
                "allowSingleLine": false
            }
        ],
        "camelcase": 0,
        "comma-spacing": [
            2,
            {
                "before": false,
                "after": true
            }
        ],
        "comma-style": 0,
        "computed-property-spacing": 0,
        "consistent-this": 0,
        "eol-last": 0,
        "func-names": 0,
        "func-style": 0,
        "id-length": 0,
        "indent": 2,
        "key-spacing": 0,
        "lines-around-comment": 0,
        "linebreak-style": 0,
        "max-nested-callbacks": 0,
        "new-cap": 0,
        "new-parens": 0,
        "newline-after-var": 0,
        "no-array-constructor": 0,
        "no-continue": 0,
        "no-inline-comments": 0,
        "no-lonely-if": 0,
        "no-mixed-spaces-and-tabs": 0,
        "no-multiple-empty-lines": [
            1,
            {
                "max": 2
            }
        ],
        "no-nested-ternary": 1,
        "no-new-object": 1,
        "no-spaced-func": 0,
        "no-ternary": 0,
        "no-trailing-spaces": 0,
        "no-underscore-dangle": 0,
        "no-unneeded-ternary": 0,
        "object-curly-spacing": 0,
        "one-var": 0,
        "operator-assignment": 0,
        "operator-linebreak": [
            2,
            "after"
        ],
        "padded-blocks": 0,
        "quote-props": 0,
        "quotes": [
            2,
            "single"
        ],
        "id-match": 0,
        "semi-spacing": 0,
        "semi": [
            2,
            "always"
        ],
        "sort-vars": 0,
        "space-after-keywords": 0,
        "space-before-blocks": 0,
        "space-before-function-paren": 0,
        "space-in-parens": 0,
        "space-infix-ops": 2,
        "space-return-throw-case": 0,
        "space-unary-ops": 0,
        "spaced-comment": 0,
        "wrap-regex": 0,
        "arrow-parens": 0,
        "arrow-spacing": 0,
        "constructor-super": 0,
        "generator-star-spacing": 0,
        "no-class-assign": 2,
        "no-const-assign": 2,
        "no-this-before-super": 2,
        "no-var": 1,
        "object-shorthand": 0,
        "prefer-const": 1,
        "prefer-spread": 0,
        "prefer-reflect": 0,
        "require-yield": 0,
        "max-depth": 0,
        "max-len": 0,
        "max-params": 0,
        "max-statements": 0,
        "no-bitwise": 0,
        "no-plusplus": 0
    }
}
